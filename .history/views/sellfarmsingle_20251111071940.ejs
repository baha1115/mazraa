    });
  }

  function closeLB(){
    if (lbSwiper){ try{ lbSwiper.destroy(true,true); }catch(_){ } lbSwiper = null; }
    lb.hidden = true;
    lb.setAttribute('aria-hidden','true');
    document.removeEventListener('keydown', onKey);
  }

  function onKey(e){ if (e.key === 'Escape') closeLB(); }

