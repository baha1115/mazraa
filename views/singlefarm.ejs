<%
function assetPath(u){
  u = (u||'').toString().trim();
  // 1) Ø§ØªØ±Ùƒ Ø§Ù„Ù€data: URI ÙƒÙ…Ø§ Ù‡Ùˆ
  if (/^data:/i.test(u)) return u;

  // 2) Ø¥Ù† ÙƒØ§Ù† URL ÙƒØ§Ù…Ù„
  if (/^https?:\/\//i.test(u)) return u;

  // 3) ØªØµØ­ÙŠØ­ Ø§Ù„Ù…Ø³Ø§Ø±Ø§Øª Ø§Ù„Ù…Ø­Ù„ÙŠØ©
  u = u.replace(/\\/g,'/');                 // backslashes â†’ slashes
  u = u.replace(/^\/?Public\//, '/public/') // Public â†’ public
       .replace(/\/assests\//gi, '/assets/');

  // 4) Ù…Ø³Ø§Ø±Ø§Øª Ù…Ù‚Ø¨ÙˆÙ„Ø© ÙƒÙ…Ø§ Ù‡ÙŠ
  if (u.startsWith('/uploads/')) return u;
  if (u.startsWith('/public/'))  return u;
  if (u.startsWith('/assets/'))  return '/public' + u;

  // 5) Ø£ÙŠ Ø´ÙŠØ¡ ÙŠØ¨Ø¯Ø£ Ø¨Ù€/ Ù†Ø¶ÙŠÙ Ù„Ù‡ /public
  if (u.startsWith('/')) return '/public' + u;

  // 6) ÙˆØ¥Ù„Ø§ Ù†Ø¹ØªØ¨Ø±Ù‡ Ù…Ù„ÙØ§Ù‹ Ù…Ø­Ù„ÙŠØ§Ù‹ Ø¯Ø§Ø®Ù„ uploads Ø£Ùˆ public Ø­Ø³Ø¨ ØªÙ†Ø¸ÙŠÙ…Ùƒ
  return '/uploads/' + u; // Ø£Ùˆ '/public/' + u Ø¥Ø°Ø§ ÙƒØ§Ù†Øª Ø£ØµÙˆÙ„Ùƒ Ù‡Ù†Ø§Ùƒ
}
